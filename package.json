{
  "name": "tini-app-jsapi-plugin",
  "version": "1.0.2",
  "description": "Tini App JSApi Plugin is a typescript server plugin that adds intellisense to Tini App JSApi.",
  "keywords": [
    "tiki",
    "tiniapp",
    "miniapp",
    "miniprogram"
  ],
  "author": "lamvd0101",
  "repository": {
    "type": "git",
    "url": "https://github.com/lamvd0101/tini-app-jsapi-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/lamvd0101/tini-app-jsapi-plugin/issues"
  },
  "main": "./dist",
  "scripts": {
    "prepare": "yarn clean && yarn build",
    "clean": "yarn rimraf dist && yarn rimraf out",
    "build": "webpack",
    "watch": "webpack --watch",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "publish": "yarn clean && yarn build && npm publish"
  },
  "dependencies": {
    "typescript": "^4.6.4"
  },
  "devDependencies": {
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.3.0",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2"
  }
}
